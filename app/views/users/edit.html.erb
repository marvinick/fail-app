<div class="form">
  <center>Enter your info here</center>
  <hr/>
  <%= form_for @user, :html => {:multipart => true} do |f| %>
      <div class='control-group'>
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-register" %>
      </div>
      <div class='control-group'>
        <%= f.label :email %>
        <%= f.text_field :email, class: "form-register" %>
      </div>
       <div class='control-group'>
        <%= f.label :password %>
        <%= f.password_field :password, class: "form-register" %>
      </div>
    <hr/>
      <div class='control-group'>
        <%= f.label :job_title %>
        <%= f.text_field :job_title, class: "form-register" %>
      </div>
      <div class='control-group'>
        <%= f.label :education %>
        <%= f.text_field :education , class: "form-register" %>
      </div>
      <div class='control-group'>
        <%= f.label :birthday %>
        <%= f.text_field :birthday, class: "form-register" %>
      </div>
      <div class='control-group'>
        <%= f.label :story %>
        <%= f.text_area :story, class: "form-register" %>
      </div>
      <div class='control-group'>
        <%= f.file_field :image, class: "form-register"  %>
      </div>
      <div class='control-group'>
        <%= f.file_field :image2, class: "form-register"  %>
      </div>
      <div class='control-group'>
        <%= f.file_field :image3, class: "form-register"  %>
      </div>
      <div class='control-group'>
        <%= f.file_field :image4, class: "form-register"  %>
      </div>
      <div class='control-group'>
        <%= f.file_field :image5, class: "form-register"  %>
      </div>
      <div class='control-group>'>
        <%= f.label :remote_image_url, "or image URL" %>
        <%= f.text_field :remote_image_url %>
      </div>

      <hr/>
    <center><%= f.submit @user.new_record? ? "Sign up" : "Update", class: 'btn btn-success btn-lg' %></center>
    <% end %>

</div>